<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
    <servlet>
        <servlet-name>ProxyRedirect</servlet-name>
        <servlet-class>it.intecs.pisa.proxy.ProxyRedirect</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Tools</servlet-name>
        <servlet-class>it.intecs.pisa.tools.Tools</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ProxyRedirect</servlet-name>
        <url-pattern>/ProxyRedirect</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Tools</servlet-name>
        <url-pattern>/Tools</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>testCenter.html</welcome-file>
        </welcome-file-list>
    </web-app>