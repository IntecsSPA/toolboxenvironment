<?xml version="1.0" encoding="UTF-8"?>

<inputInterface xmlns="http://gisClient.pisa.intecs.it/gisClient">
    <section name="TOOLBOX Import Services">
        <group label="Load the services zip file:">
          <input type="file" name="zipServices" hideLabel="false" id="zipServices" 
           optional="true"
           size="300"
           autoUploadURL="rest/gui/storeData"
           iconImage="upload-icon"
           blankText="Load Services Group Zip File"
           label="Select Services Group Zip File"
           iconWait="images/loaderFile.gif"
           iconFailure="images/fail.png"
           iconSuccess="images/success.png"
           submitLabel="Import Services"
           editAreaID="test"/>
           <input type="editarea" name="test" id="test"  hidden="true" hideLabel="true"
                adaptButtonLabel="Adapt to screen" syntax="html"
                height="screen.height/1.8"/>
           
        </group>
    </section>
    <requestInformations>
        <buttons>
            <button name="importServicesButton" id="importServicesButton" label="Import Services" iconImage="" onclick="importServices"/>
        </buttons>
    </requestInformations>
</inputInterface>
