	<SOAP-ENV:Body>
		<exportDescriptor xmlns="http://pisa.intecs.it/mass/toolbox/exportDescriptor" xmlns:com="http://pisa.intecs.it/mass/toolbox/common" xmlns:dsc="http://pisa.intecs.it/mass/toolbox/serviceDescriptor" xmlns:mtbs="http://pisa.intecs.it/mass/toolbox/xmlScript">
			<service queuing="false" serviceName="newTestService" serviceSchema="" suspendMode="soft" wsdlTargetNS="test.pisa.intecs.it" xmlns="http://pisa.intecs.it/mass/toolbox/serviceDescriptor" xmlns:com="http://pisa.intecs.it/mass/toolbox/common" xmlns:mtbs="http://pisa.intecs.it/mass/toolbox/xmlScript">
				<abstract>Test service</abstract>
				<description>This service is used to test the TOOLBOX installation.</description>
				<schemas/>
				<operations>
					<operation name="testOperation" type="synchronous">
						<wsdlInfo>
							<requestMessage name="aa" ns="aa"/>
							<responseMessage name="aa" ns="aa"/>
						</wsdlInfo>
						<script soapAction="test">
							<mtbs:sequence xmlns:mtbs="http://pisa.intecs.it/mass/toolbox/xmlScript">
								<mtbs:xml attributePrefix="attribute$" textTag="evaluate">
									<testService>
										<helloWorld>The TOOLBOX has been correctly installed.</helloWorld>
									</testService>
								</mtbs:xml>
							</mtbs:sequence>
						</script>
					</operation>
				</operations>
