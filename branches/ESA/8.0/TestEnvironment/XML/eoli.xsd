lStringType">										<!-- <xsd:annotation>											<xsd:documentation>ISO 19115 shortName of element 360 (Citation and responsible party information).</xsd:documentation>										</xsd:annotation> -->									</xsd:element>								</xsd:sequence>							</xsd:complexType>						</xsd:element>						<xsd:element name="idAbs" type="CharacterString" minOccurs="0">							<!-- <xsd:annotation>								<xsd:documentation>ISO 19115  shortName of element 25 (Abstract). </xsd:documentation>							</xsd:annotation> -->						</xsd:element>						<xsd:element ref="idStatus" minOccurs="0"/>						<xsd:element name="dataExt">							<!-- <xsd:annotation>								<xsd:documentation>ISO19115 element 45 (extent). </xsd:documentation>							</xsd:annotation> -->							<xsd:complexType>								<xsd:complexContent>									<xsd:restriction base="DataExtType">										<xsd:sequence>											<xsd:element name="tempEle" type="EX_TemporalExtendType" minOccurs="0">												<!-- <xsd:annotation>													<xsd:documentation>ISO 19115 shortName of element 337 (temporalElement)</xsd:documentation>												</xsd:annotation> -->											</xsd:element>											<xsd:element name="geoEle" minOccurs="0">												<!-- <xsd:annotation>													<xsd:documentation>ISO 19115 shortName of element 336 (geographicElement)</xsd:documentation>												</xsd:annotation> -->												<xsd:complexType>													<xsd:choice>														<xsd:sequence>															<xsd:element name="polygon">																<!-- <xsd:annotation>																	<xsd:documentation>Geographic Polygon element. ISO 19115 short name of element 342 (polygon). The type is defined ISO 19107</xsd:documentation>																</xsd:annotation> -->																<xsd:complexType>																	<xsd:sequence>																		<xsd:element ref="coordinates"/>																	</xsd:sequence>																</xsd:complexType>															</xsd:element>															<xsd:element name="scCenter" minOccurs="0">																<!-- <xsd:annotation>																	<xsd:documentation>This represents the Scene Center element.</xsd:documentation>																</xsd:annotation> -->																<xsd:complexType>																	<xsd:sequence>																		<xsd:element ref="coordinates"/>																	</xsd:sequence>																</xsd:complexType>															</xsd:element>														</xsd:sequence>													</xsd:choice>												</xsd:complexType>											</xsd:element>										</xsd:sequence>									</xsd:restriction>								</xsd:complexContent>							</xsd:complexType>						</xsd:element>					</xsd:sequence>				</xsd:complexType>			</xsd:element>			<xsd:element name="contInfo" minOccurs="0">				<!-- <xsd:annotation>					<xsd:documentation>ISO 19115 shortName of element 16 (contentInfo).</xsd:documentation>				</xsd:annotation> -->				<xsd:complexType>					<xsd:sequence>						<xsd:element name="attDesc">							<!-- <xsd:annotation>								<xsd:documentation>ISO 19115 shortName of element 240 (attributeDescription).</xsd:documentation>							</xsd:annotation> -->							<xsd:complexType>								<xsd:sequence>									<xsd:element name="typeName" fixed="imageType" nillable="false">										<!-- <xsd:annotation>											<xsd:documentation>ISO 19103.</xsd:documentation>										</xsd:annotation> -->										<xsd:simpleType>											<xsd:restriction base="CharacterString">												<xsd:enumeration value="imageType"/>											</xsd:restriction>										</xsd:simpleType>									</xsd:element>									<xsd:element name="attTypes">										<!-- <xsd:annotation>											<xsd:documentation>ISO 19103.</xsd:documentation>										</xsd:annotation> -->										<xsd:complexType>											<xsd:sequence>												<xsd:element name="attName" fixed="imageQuality" nillable="false">													<!-- <xsd:annotation>														<xsd:documentation>ISO 1