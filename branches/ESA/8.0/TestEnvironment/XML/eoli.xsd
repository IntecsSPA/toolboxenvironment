<?xml version="1.0" encoding="UTF-8"?><!-- edited with XML Spy v4.4 U (http://www.xmlspy.com) by Simone Gianfranceschi (ESA - European Space Agency) --><!-- 	File			:	eoli.xsd 		File Type	:	W3C Schema 		Date		:	02 December 2003 		Abstract	:	MUIS Schema 		Uses		:	 		Author		:	Nencioni Patrizia 		Issue		:	V1.7 --><xsd:schema targetNamespace="http://earth.esa.int/XML/eoli" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns="http://earth.esa.int/XML/eoli" elementFormDefault="qualified">	<!-- ==================== Element Section ====================== -->	<!-- ======================================================== -->	<xsd:element name="searchRequest" type="SearchRequestType">		<!-- <xsd:annotation>			<xsd:documentation>Inventory Search Request Root Element as defined in the CG_QueryRequest of the OpenGIS - Catalog Interface Implementation Specification (Version 1.0).</xsd:documentation>		</xsd:annotation> -->	</xsd:element>	<xsd:element name="beginEnd">		<!-- <xsd:annotation>			<xsd:documentation>ISO 19115 short name of element Start and End Date element.</xsd:documentation>		</xsd:annotation> -->		<xsd:complexType>			<xsd:sequence>				<xsd:element name="begin" type="xsd:dateTime">					<!-- <xsd:annotation>						<xsd:documentation>Start date in ISO 8601 format. This element can contain: date (format CCYY-MM-DD) or dateTime (format CCYY-MM-DDThh:mm:ss)</xsd:documentation>					</xsd:annotation> -->				</xsd:element>				<xsd:element name="end" type="xsd:dateTime">					<!-- <xsd:annotation>						<xsd:documentation>End data in ISO 8601 format.  This element can contain: date (format CCYY-MM-DD) or dateTime (format CCYY-MM-DDThh:mm:ss)</xsd:documentation>					</xsd:annotation> -->				</xsd:element>			</xsd:sequence>		</xsd:complexType>	</xsd:element>	<xsd:element name="collectionId" type="NonNullStringType">		<!-- <xsd:annotation>			<xsd:documentation>This field specified the search space for the query request. TargetService Root Element.  Its name comes from the CG_QueryRequest of the OpenGIS - Catalog Interface Implementation Specification (Version 1.0). For this element, use the following syntax: Provider.Facility.CollectionId where Provider, Facility and CollectionId elements are defined by the data provider.</xsd:documentation>		</xsd:annotation> -->	</xsd:element>	<xsd:element name="cursor" type="xsd:positiveInteger" nillable="false">		<!-- <xsd:annotation>			<xsd:documentation>This field identifies: in the query request, the first result set entry to be returned in the query response; in the query response, the last item in the result set that was returned in the retrieved data set as defined in the CG_QueryRequest and the CG_QueryResponse of the OpenGIS - Catalog Interface Implementation Specification (Version 1.0).</xsd:documentation>		</xsd:annotation> -->	</xsd:element>	<xsd:element name="presentation" type="PresentationType" nillable="false">		<!-- <xsd:annotation>			<xsd:documentation>This field is only valid when results are requested to be returned directly in the query response. This parameter informs the server which of the attributes in the result set elements are to be returned to the client as defined in the CG_QueryRequest of the OpenGIS - Catalog Interface Implementation Specification (Version 1.0). This field can take on only the following values: brief, summary, full and browse.</xsd:documentation>		</xsd:annotation> -->	</xsd:element>	<xsd:element name="status" type="StatusType" nillable="false">		<!-- <xsd:annotation>			<xsd:documentation>This parameter conveys the success or failure of the query request as defined in the CG_QueryResponse of the OpenGIS - Catalog Interface Implementation Specification (Version 1.0).</xsd:documentation>		</xsd:annotation> -->	</xsd:element>	<xsd:element name="frame" type="xsd:positiveInteger" nillable="false">		<!-- <xsd:annotation>			<xsd:documentation>ISO 19130 Frame field.</xsd:documentation>		</xsd:annotation> -->	</xsd:element>	<xsd:element name="track" type="xsd:positiveInteger" nillable="false">		<!-- <xsd:annotation>			<xsd:documentation>ISO 19130 Track field.</xsd:documentation>		</xsd:annotation> -->	</xsd:element>	<xsd:element name="orbit" type="xsd:positiveInteger" nillable="false">		<!-- <xsd:annotation>			<xsd:documentation>Orbit field as defined in CEOS CIP 2.4 as "OrbitNumber" element.</xsd:documentation>		</xsd:annotation> -->	</xsd:element>	<xsd:element name="presentRequest">		<!-- <xsd:annotation>			<xsd:documentation>Inventory Retrieve Request Root Element as defined in the CG_PresentRequest of the OpenGIS - Catalog Interface Implementation Specification (Version 1.0).</xsd:documentation>		</xsd:annotation> -->		<xsd:complexType>			<xsd:sequence>				<xsd:element name="idCitation">					<!-- <xsd:annotation>						<xsd:documentation>Product Identifier element. It is limited to a single value with an attribute named operator. ISO 19115 short name of element 24 (Identification Information).</xsd:documentation>					</xsd:annotation> -->					<xsd:complexType>						<xsd:sequence>							<xsd:element ref="resTitle" maxOccurs="unbounded">								<!-- <xsd:annotation>									<xsd:documentation>ISO 19115 shortName of element 360 (Citation and responsible party information)</xsd:documentation>								</xsd:annotation> -->							</xsd:element>						</xsd:sequence>					</xsd:complexType>				</xsd:element>				<xsd:element ref="presentation"/>				<xsd:element ref="collectionId"/>			</xsd:sequence>		</xsd:complexType>	</xsd:element>	<xsd:element name="platfSNm" type="NonNullStringType">		<!-- <xsd:annotation>			<xsd:documentation>This represents a short name of the Platform as defined in the FGDC extensions for Remote Sensing Data document as the shortName of the "Platform_Short_Name" element. This element can take on only the CEOS CIP valid Value for the Platform element.</xsd:documentation>		</xsd:annotation> -->	</xsd:element>	<xsd:element name="platfSer" type="CharacterString">		<!-- <xsd:annotation>			<xsd:documentation>This represents a letter and/or a numbers applied to the platform as defined in the FGDC extensions for Remote Sensing Data document as the shortName of the "Platform_Serial_Identifier" element. This element can take on only the CEOS CIP valid Value for the Platform element.</xsd:documentation>		</xsd:annotation> -->	</xsd:element>	<xsd:element name="instShNm" type="NonNullStringType">		<!-- <xsd:annotation>			<xsd:documentation>SensorId element as defined in the FGDC extensions for Remote Sensing Data document as "Instrument_Short_Name" element. This element can take on only the CEOS CIP valid Value for the SensorId element.</xsd:documentation>		</xsd:annotation> -->	</xsd:element>	<xsd:element name="resTitle" type="NonNullStringType">		<!-- <xsd:annotation>			<xsd:documentation>ISO 19115 shortName of element 360 (Citation and responsible party information).</xsd:documentation>		</xsd:annotation> -->	</xsd:element>	<xsd:element name="response" type="ResponseType">		<!-- <xsd:annotation>			<xsd:documentation>Inventory Search Response Root Element as defined in the CG_QueryResponse of the OpenGIS - Catalog Interface Implementation Specification (Version 1.0).</xsd:documentation>		</xsd:annotation> -->	</xsd:element>	<xsd:element name="coordinates" type="CoordinatesType" nillable="false">		<!-- <xsd:annotation>			<xsd:documentation>It maps gml211 Coordinates definition. It models a list of points coordinates according the following format: points are separated by blank, point's coordinates lat and long are separated by comma (,). The decimal point (.) is used to separate decimals.</xsd:documentation>		</xsd:annotation> -->	</xsd:element>	<xsd:element name="idStatus" type="IdStatusEnumType">		<!-- <xsd:annotation>			<xsd:documentation>This represents the ProductStatus element. Its name comes from ISO 19115 document as "Identification Information" (shortName of the 28 element). 008 (potential): visible/potential (can be acquired, but is not planned); 006 (required): planning of these data has been requested; 005 (planned): planned; 007 (underDevelopment): acquired; 001 (completed): produced (the relevant level of this item has been produced and is readily available).</xsd:documentation>		</xsd:annotation> -->	</xsd:element>	<!-- ================== Simple Type Section ==================== -->	<!-- ======================================================= -->	<xsd:simpleType name="DateTimeTypes">		<xsd:union memberTypes="xsd:date xsd:dateTime"/>	</xsd:simpleType>	<xsd:simpleType name="NonNullStringType">		<!-- <xsd:annotation>			<xsd:documentation>Not null string type.</xsd:documentation>		</xsd:annotation> -->		<xsd:restriction base="CharacterString">			<xsd:minLength value="1"/>		</xsd:restriction>	</xsd:simpleType>	<xsd:simpleType name="CharacterString">		<!-- <xsd:annotation>			<xsd:documentation>Character string type.</xsd:documentation>		</xsd:annotation> -->		<xsd:restriction base="xsd:string">			<xsd:minLength value="0" fixed="false"/>		</xsd:restriction>	</xsd:simpleType>	<xsd:simpleType name="LongitudeType">		<!-- <xsd:annotation>			<xsd:documentation>Type of a longitude value.</xsd:documentation>		</xsd:annotation> -->		<xsd:restriction base="xsd:float">			<xsd:minInclusive value="-180.0"/>			<xsd:maxInclusive value="180.0"/>		</xsd:restriction>	</xsd:simpleType>	<xsd:simpleType name="LatitudeType">		<!-- <xsd:annotation>			<xsd:documentation>Type of a latitude value.</xsd:documentation>		</xsd:annotation> -->		<xsd:restriction base="xsd:float">			<xsd:minInclusive value="-90.0"/>			<xsd:maxInclusive value="90.0"/>		</xsd:restriction>	</xsd:simpleType>	<xsd:simpleType name="cloudCoveragePercType">		<!-- <xsd:annotation>			<xsd:documentation>Type of a cloudCoveragePerc value.</xsd:documentation>		</xsd:annotation> -->		<xsd:restriction base="xsd:float">			<xsd:minInclusive value="0.0"/>			<xsd:maxInclusive value="100.0"/>		</xsd:restriction>	</xsd:simpleType>	<xsd:simpleType name="PresentationType">		<!-- <xsd:annotation>			<xsd:documentation>Type of a presentation element.</xsd:documentation>		</xsd:annotation> -->		<xsd:restriction base="CharacterString">			<xsd:enumeration value="brief">								<!-- <xsd:annotation> 					<xsd:documentation source="as defined in CG_PresentationDescriptorType of the OpenGIS - Catalog Interface Implementation Specification (Version 1.0)"/> 				</xsd:annotation> 				-->			</xsd:enumeration>			<xsd:enumeration value="summary">							<!-- <xsd:annotation> 					<xsd:documentation source="as defined in CIP 2.4"/> 				</xsd:annotation> 				-->			</xsd:enumeration>			<xsd:enumeration value="full">							<!-- <xsd:annotation> 					<xsd:documentation source="as defined in CG_PresentationDescriptorType of the OpenGIS - Catalog Interface Implementation Specification (Version 1.0)"/> 				</xsd:annotation> 				-->			</xsd:enumeration>			<xsd:enumeration value="browse">							<!-- <xsd:annotation> 					<xsd:documentation source="as defined in CIP 2.4"/> 				</xsd:annotation> 				-->			</xsd:enumeration>		</xsd:restriction>	</xsd:simpleType>	<xsd:simpleType name="StatusType">		<!-- <xsd:annotation>			<xsd:documentation>Type of the status element. The status is a code list for representing the current status of a resource or request. The valid values for this type come from the CG_QueryResponse of the OpenGIS - Catalog Interface Implementation Specification (Version 1.0) </xsd:documentation>		</xsd:annotation> -->		<xsd:restriction base="CharacterString">			<xsd:enumeration value="success">				<!-- 				<xsd:annotation> 					<xsd:documentation source="The request has been processed without error (as defined in the CG_QueryResponse of the OpenGIS - Catalog Interface Implementation Specification Version 1.0)"/> 				</xsd:annotation> 				-->			</xsd:enumeration>			<xsd:enumeration value="failure">				<!-- 				<xsd:annotation> 					<xsd:documentation source="request could not be completed due to errors being encountered (as defined in the CG_QueryResponse of the OpenGIS - Catalog Interface Implementation Specification Version 1.0)"/> 				</xsd:annotation> 				-->			</xsd:enumeration>		</xsd:restriction>	</xsd:simpleType>	<xsd:simpleType name="RoleEnumType">		<!-- <xsd:annotation>			<xsd:documentation>Type of Role element.</xsd:documentation>		</xsd:annotation> -->		<xsd:restriction base="NonNullStringType">			<xsd:enumeration value="002"/>			<xsd:enumeration value="006"/>			<xsd:enumeration value="009"/>		</xsd:restriction>	</xsd:simpleType>	<xsd:simpleType name="IdStatusEnumType">		<!-- <xsd:annotation>			<xsd:documentation>Type of a IdStatus element.</xsd:documentation>		</xsd:annotation> -->		<xsd:restriction base="NonNullStringType">			<xsd:enumeration value="001">				<!-- 				<xsd:annotation> 					<xsd:documentation source="(complited): producted (the relevant level of this item has been producted and is readily available)."/> 				</xsd:annotation> 				-->			</xsd:enumeration>			<xsd:enumeration value="005">				<!-- 				<xsd:annotation> 					<xsd:documentation source="(planned): planned."/> 				</xsd:annotation> 				-->			</xsd:enumeration>			<xsd:enumeration value="006">				<!-- 				<xsd:annotation> 					<xsd:documentation source="(required): planning of this data has been requested."/> 				</xsd:annotation> 				-->			</xsd:enumeration>			<xsd:enumeration value="007">				<!-- 				<xsd:annotation> 					<xsd:documentation source="(underDevelopted): acquired."/> 				</xsd:annotation> 				-->			</xsd:enumeration>			<xsd:enumeration value="008">				<!-- 				<xsd:annotation> 					<xsd:documentation source="(potential): visible/potential (can be acquired, but not is planned)."/> 				</xsd:annotation> 				-->			</xsd:enumeration>		</xsd:restriction>	</xsd:simpleType>	<xsd:simpleType name="DegreeType">		<!-- <xsd:annotation>			<xsd:documentation>Type of a degree value.</xsd:documentation>		</xsd:annotation> -->		<xsd:restriction base="xsd:float">			<xsd:minInclusive value="0.0"/>			<xsd:maxInclusive value="360.0"/>		</xsd:restriction>	</xsd:simpleType>	<xsd:simpleType name="ContentTypeEnum">		<!-- <xsd:annotation>			<xsd:documentation>Type of contentInfo element. Allowed value is 001=image.</xsd:documentation>		</xsd:annotation> -->		<xsd:restriction base="NonNullStringType">			<xsd:enumeration value="001"/>		</xsd:restriction>	</xsd:simpleType>	<xsd:simpleType name="OrbitDirectionEnumType">		<!-- <xsd:annotation>			<xsd:documentation>Type of OrbitDirectionEnum element. </xsd:documentation>		</xsd:annotation> -->		<xsd:restriction base="CharacterString">			<xsd:enumeration value="0">				<!-- <xsd:annotation>					<xsd:documentation source="Ascending Direction"/>				</xsd:annotation> -->			</xsd:enumeration>			<xsd:enumeration value="1">				<!-- <xsd:annotation>					<xsd:documentation source="Descending Direction."/>				</xsd:annotation> -->			</xsd:enumeration>		</xsd:restriction>	</xsd:simpleType>	<xsd:simpleType name="PolarisationEnumType">		<!-- <xsd:annotation>			<xsd:documentation>Type of Polarisation element. </xsd:documentation>		</xsd:annotation> -->		<xsd:restriction base="NonNullStringType">			<xsd:enumeration value="HH"/>			<xsd:enumeration value="VV"/>			<xsd:enumeration value="HV"/>			<xsd:enumeration value="VH"/>		</xsd:restriction>	</xsd:simpleType>	<xsd:simpleType name="FullSceneEnumType">		<!-- <xsd:annotation>			<xsd:documentation>Type of FullSceneEnum element. </xsd:documentation>		</xsd:annotation> -->		<xsd:restriction base="NonNullStringType">			<xsd:enumeration value="F"/>			<xsd:enumeration value="P"/>		</xsd:restriction>	</xsd:simpleType>	<xsd:simpleType name="CoordinatesType">		<!-- <xsd:annotation>			<xsd:documentation>Type of coordinates element.</xsd:documentation>		</xsd:annotation> -->		<xsd:restriction base="NonNullStringType"/>	</xsd:simpleType>	<!-- ================== Complex Type Section =================== -->	<!-- ======================================================== -->	<xsd:complexType name="SearchRequestType">		<!-- <xsd:annotation>			<xsd:documentation>Type of the searchRequest element.</xsd:documentation>		</xsd:annotation> -->		<xsd:sequence>			<xsd:element name="simpleQuery" type="SimpleQueryType">				<!-- <xsd:annotation>					<xsd:documentation>Query Element Root request.</xsd:documentation>				</xsd:annotation> -->			</xsd:element>			<xsd:element name="resultType" nillable="false">				<!-- <xsd:annotation>					<xsd:documentation>Type of data to be returned in a query response message and the behaviour of the message response as defined in the CG_QueryRequest of the OpenGIS - Catalog Interface Implementation Specification (Version 1.0)</xsd:documentation>				</xsd:annotation> -->				<xsd:simpleType>					<xsd:restriction base="NonNullStringType">						<xsd:enumeration value="results"/>						<xsd:enumeration value="hits"/>					</xsd:restriction>				</xsd:simpleType>			</xsd:element>			<xsd:element name="iteratorSize" type="xsd:positiveInteger" nillable="false" minOccurs="0">				<!-- <xsd:annotation>					<xsd:documentation>This field indicates the maximum number of the result set entries to be returned in the query response.  Its name comes from the CG_QueryRequest of the OpenGIS - Catalog Interface Implementation Specification (Version 1.0)</xsd:documentation>				</xsd:annotation> -->			</xsd:element>			<xsd:element ref="cursor" minOccurs="0"/>			<xsd:element ref="presentation" minOccurs="0">				<!-- <xsd:annotation>					<xsd:documentation>This field is only valid when results are requested to be returned directly in the query response. This parameter informs the server which of the attributes in the result set elements are to be returned to the client as defined in the CG_QueryRequest of the OpenGIS - Catalog Interface Implementation Specification (Version 1.0). This field can take on only the following values: brief, summary, full and browse.</xsd:documentation>				</xsd:annotation> -->			</xsd:element>			<xsd:element ref="collectionId">				<!-- <xsd:annotation>					<xsd:documentation>This field specified the search space for the query request. TargetService Root Element.  Its name comes from the CG_QueryRequest of the OpenGIS - Catalog Interface Implementation Specification (Version 1.0) document. For this element, use the following syntax: Provider.Facility.CollectionId where Provider, Facility and CollectionId elements are defined by the data provider.</xsd:documentation>				</xsd:annotation> -->			</xsd:element>		</xsd:sequence>	</xsd:complexType>	<xsd:complexType name="SimpleQueryType">		<!-- <xsd:annotation>			<xsd:documentation>Type of the simpleQuery element.</xsd:documentation>		</xsd:annotation> -->		<xsd:sequence>			<xsd:element name="dataExt" minOccurs="0">				<!-- <xsd:annotation>					<xsd:documentation>Spatial and temporal condition define as per ISO19115 element 45 (extent). The Spatial Condition is limited to single BoundingRectangle with an attribute named operator. The Temporal Coverage is limited to single temporal range.</xsd:documentation>				</xsd:annotation> -->				<xsd:complexType>					<xsd:complexContent>						<xsd:restriction base="DataExtType">							<xsd:sequence>								<xsd:element name="geoEle" minOccurs="0">									<!-- <xsd:annotation>										<xsd:documentation>ISO 19115 metadata element 336 (geographicElement).</xsd:documentation>									</xsd:annotation> -->									<xsd:complexType>										<xsd:choice>											<xsd:element name="geoBndBox">												<!-- <xsd:annotation>													<xsd:documentation>ISO 19115 metadata element 343 (EX_GeographicBoundingBox)</xsd:documentation>												</xsd:annotation> -->												<xsd:complexType>													<xsd:complexContent>														<xsd:extension base="EX_GeographicBoundingBoxType"/>													</xsd:complexContent>												</xsd:complexType>											</xsd:element>										</xsd:choice>										<xsd:attribute name="operator">											<xsd:simpleType>												<xsd:restriction base="CharacterString">													<xsd:enumeration value="OVERLAP"/>												</xsd:restriction>											</xsd:simpleType>											<!-- 											<xsd:annotation> 												<xsd:documentation source='The operator attribute can take on only one value (OVERLAP). The value of the operator attribute comes from the "BNF definition of OGC_Common Query Language" of the OpenGIS - Catalog Interface Implementation Specification (Version 1.0)'/> 											</xsd:annotation> 											-->										</xsd:attribute>									</xsd:complexType>								</xsd:element>								<xsd:element name="tempEle" minOccurs="0">									<!-- <xsd:annotation>										<xsd:documentation>ISO 19115 metadata element 337 (temporalElement).</xsd:documentation>									</xsd:annotation> -->									<xsd:complexType>										<xsd:sequence>											<xsd:element name="exTemp">												<!-- <xsd:annotation>													<xsd:documentation>ISO 19115 shortName of element 351 (temporal extent).</xsd:documentation>												</xsd:annotation> -->												<xsd:complexType>													<xsd:choice>														<xsd:element name="beginEnd">															<!-- <xsd:annotation>																<xsd:documentation>ISO 19115 short name of element Start and End Date element.</xsd:documentation>															</xsd:annotation> -->															<xsd:complexType>																<xsd:sequence>																	<xsd:element name="begin" type="xsd:date" nillable="false">																		<!-- <xsd:annotation>																			<xsd:documentation>Start date in ISO 8601 format. This element can contain: date (format CCYY-MM-DD).</xsd:documentation>																		</xsd:annotation> -->																	</xsd:element>																	<xsd:element name="end" type="xsd:date" nillable="false">																		<!-- <xsd:annotation>																			<xsd:documentation>End data in ISO 8601 format.  This element can contain: date (format CCYY-MM-DD).</xsd:documentation>																		</xsd:annotation> -->																	</xsd:element>																</xsd:sequence>															</xsd:complexType>														</xsd:element>													</xsd:choice>												</xsd:complexType>											</xsd:element>										</xsd:sequence>										<xsd:attribute name="operator">											<xsd:simpleType>												<xsd:restriction base="CharacterString">													<xsd:enumeration value="OVERLAP"/>												</xsd:restriction>											</xsd:simpleType>											<!-- 											<xsd:annotation> 												<xsd:documentation source='The operator attribute can take on only one value (OVERLAP). The value of the operator attribute comes from the "BNF definition of OGC_Common Query Language" of the OpenGIS - Catalog Interface Implementation Specification (Version 1.0)'/> 											</xsd:annotation> 											-->										</xsd:attribute>									</xsd:complexType>								</xsd:element>							</xsd:sequence>						</xsd:restriction>					</xsd:complexContent>				</xsd:complexType>			</xsd:element>			<xsd:element name="satelliteDomainConditions" minOccurs="0">				<!-- <xsd:annotation>					<xsd:documentation>Satellite domain conditions element root.</xsd:documentation>				</xsd:annotation> -->				<xsd:complexType>					<!-- <xsd:annotation>						<xsd:documentation>type of the satelliteExtendedConditions element. </xsd:documentation>					</xsd:annotation> -->					<xsd:sequence>						<xsd:element name="plaInsIdCondition" minOccurs="0">							<!-- <xsd:annotation>								<xsd:documentation>plaInsIdCondition Identification element Root. This element is limited to a single value with an attribute named operator.</xsd:documentation>							</xsd:annotation> -->							<xsd:complexType>								<!-- <xsd:annotation>									<xsd:documentation>Type of the plaInsIdConditionType.</xsd:documentation>								</xsd:annotation> -->								<xsd:sequence>									<xsd:element name="plaInsId">										<!-- <xsd:annotation>											<xsd:documentation>Satellite element as defined in the FGDC extensions for Remote Sensing Data document as "Platform_and_Instrument_identification" element.</xsd:documentation>										</xsd:annotation> -->										<xsd:complexType>											<xsd:sequence>												<xsd:element ref="platfSNm">													<!-- <xsd:annotation>														<xsd:documentation>This represents a short name of the Platform as defined in the FGDC extensions for Remote Sensing Data document as the shortName of the "Platform_Short_Name" element. This element can take on only the CEOS CIP valid Value for the Platform element.</xsd:documentation>													</xsd:annotation> -->												</xsd:element>												<xsd:element ref="platfSer" minOccurs="0">													<!-- <xsd:annotation>														<xsd:documentation>This represents a letter and/or a numbers applied to the platform as defined in the FGDC extensions for Remote Sensing Data document as the shortName of the "Platform_Serial_Identifier" element. This element can take on only the CEOS CIP valid Value for the Platform element.</xsd:documentation>													</xsd:annotation> -->												</xsd:element>											</xsd:sequence>										</xsd:complexType>									</xsd:element>								</xsd:sequence>								<xsd:attribute name="operator">									<xsd:simpleType>										<xsd:restriction base="CharacterString">											<xsd:enumeration value="EQUAL"/>										</xsd:restriction>									</xsd:simpleType>								</xsd:attribute>							</xsd:complexType>						</xsd:element>						<xsd:element name="cloudCoverCondition" minOccurs="0">							<!-- <xsd:annotation>								<xsd:documentation>cloudCoverageCondition root element.</xsd:documentation>							</xsd:annotation> -->							<xsd:complexType>								<xsd:sequence>									<xsd:element name="cloudCovePerc" type="cloudCoveragePercType" nillable="false">										<!-- <xsd:annotation>											<xsd:documentation>ISO 19115 shortName of the element 248 (cloudCoverPercentage).</xsd:documentation>										</xsd:annotation> -->									</xsd:element>								</xsd:sequence>								<xsd:attribute name="operator">									<xsd:simpleType>										<xsd:restriction base="CharacterString">											<xsd:enumeration value="LESS EQUAL"/>										</xsd:restriction>									</xsd:simpleType>								</xsd:attribute>							</xsd:complexType>						</xsd:element>						<xsd:element name="genericCondition" minOccurs="0" maxOccurs="unbounded">							<!-- <xsd:annotation>								<xsd:documentation>genericCondition root element.</xsd:documentation>							</xsd:annotation> -->							<xsd:complexType>								<xsd:sequence>									<xsd:element name="attributeId" type="NonNullStringType" nillable="false"/>									<xsd:element name="attributeValue" type="NonNullStringType" nillable="false" maxOccurs="unbounded"/>								</xsd:sequence>								<xsd:attribute name="operator">									<xsd:simpleType>										<xsd:restriction base="CharacterString">											<xsd:enumeration value="EQUAL"/>											<xsd:enumeration value="LESS EQUAL"/>											<xsd:enumeration value="LESS"/>											<xsd:enumeration value="GREATER"/>											<xsd:enumeration value="GREATER EQUAL"/>											<xsd:enumeration value="WITHIN"/>											<xsd:enumeration value="INTERSECTS"/>										</xsd:restriction>									</xsd:simpleType>								</xsd:attribute>							</xsd:complexType>						</xsd:element>					</xsd:sequence>				</xsd:complexType>			</xsd:element>		</xsd:sequence>	</xsd:complexType>	<xsd:complexType name="DataExtType">		<!-- <xsd:annotation>			<xsd:documentation>Type of the dataExt element.</xsd:documentation>		</xsd:annotation> -->		<xsd:sequence>			<xsd:element name="geoEle" minOccurs="0">				<!-- <xsd:annotation>					<xsd:documentation>ISO 19115 element 336 short (geographicElement)</xsd:documentation>				</xsd:annotation> -->				<xsd:complexType>					<xsd:choice>						<xsd:sequence>							<xsd:element name="polygon">								<!-- <xsd:annotation>									<xsd:documentation>geographic Polygon element as defined in ISO 19115 element 342 (polygon). The type is defined ISO 19107</xsd:documentation>								</xsd:annotation> -->								<xsd:complexType>									<xsd:sequence>										<xsd:element ref="coordinates"/>									</xsd:sequence>								</xsd:complexType>							</xsd:element>							<xsd:element name="scCenter" minOccurs="0">								<!-- <xsd:annotation>									<xsd:documentation>Scene Center element. </xsd:documentation>								</xsd:annotation> -->								<xsd:complexType>									<xsd:sequence>										<xsd:element ref="coordinates"/>									</xsd:sequence>								</xsd:complexType>							</xsd:element>						</xsd:sequence>						<xsd:element name="geoBndBox">							<!-- <xsd:annotation>								<xsd:documentation>ISO 19115 element 343 (EX_GeographicBoundingBox type)</xsd:documentation>							</xsd:annotation> -->							<xsd:complexType>								<xsd:complexContent>									<xsd:extension base="EX_GeographicBoundingBoxType"/>								</xsd:complexContent>							</xsd:complexType>						</xsd:element>					</xsd:choice>					<xsd:attribute name="operator">						<xsd:simpleType>							<xsd:restriction base="CharacterString">								<xsd:enumeration value="OVERLAP"/>							</xsd:restriction>						</xsd:simpleType>						 						<!-- <xsd:annotation> 							<xsd:documentation source='The operator attribute can take on only one value (OVERLAP). The value of the operator attribute comes from the "BNF definition of OGC_Common Query Language" of the OpenGIS - Catalog Interface Implementation Specification (Version 