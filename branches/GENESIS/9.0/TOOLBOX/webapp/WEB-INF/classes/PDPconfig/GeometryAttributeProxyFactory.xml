<?xml version="1.0" encoding="UTF-8"?>
<!-- ===========================================================================
This file contains the configuration of the factory GeometryAttributeProxyFactory.
You can add further proxies by adding a bean within the list element.

author: Benjamin Friedrich
date:   05.06.2009.
============================================================================ -->
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
       http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">

    <bean id="GeometryAttributeProxyFactory" class="net.eig.geoxacml.pdp.attribute.proxy.GeometryAttributeProxyFactory">
        <constructor-arg>
            <bean class="java.util.ArrayList">
                <constructor-arg>
                    <list>
                        <bean class="net.eig.geoxacml.pdp.attribute.proxy.GeometryAttributeProxy" />
                    <!-- <bean class="my.further.attribute.Proxy"/> -->
                    </list>
                </constructor-arg>
            </bean>
        </constructor-arg>
    </bean>
</beans>