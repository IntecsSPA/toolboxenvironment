<?xml version="1.0" encoding="UTF-8"?>

<inputInterface xmlns="http://gisClient.pisa.intecs.it/gisClient">
   <requestInformation backgroundColor="#617991"/>
   <section name="Service Info">
        <group label="Service Identification: "> 
            <input type="text" name="serviceName" label="Service Name" id="serviceName" optional="false" value="" size="52"/>
            <input type="text" name="serviceVersion" label="Service Version" id="serviceVersion" optional="false" value="" size="52"/>
            <input type="textarea" name="serviceAbstract" label="Abstract" id="serviceAbstract" optional="true" value="" cols="50" rows="5"/>
            <input type="text" name="serviceKeyWords" label="KeyWords( separated by ',')" id="serviceKeyWords" optional="true" value="" size="52"/>
            <input type="hidden" name="interfaceType" id="interfaceType" value="ordering"/>
        </group>
   </section>
   <section name="Provider Info">
        <group label="Service Provider Information">
            <input type="text" name="ProviderName" label="Provider Name" id="ProviderName" optional="true" value="" size="52"/>
            <input type="text" name="ProviderSite" label="Provider Site" id="ProviderSite" optional="true" value="" size="52"/>
        </group>
   </section>
   <section name="Contact Info">
        <group label="Service Contact Information">
            <input type="text" name="IndividualName" label="Individual Name" id="IndividualName" optional="true" value="" size="52"/>
            <input type="text" name="PositionName" label="Position Name" id="PositionName" optional="true" value="" size="52"/>
            <input type="text" name="PhoneVoice" label="Phone Voice" id="PhoneVoice" optional="true" value="" size="25"/>
             <input type="text" name="PhoneFacsimile" label="Phone Facsimile" id="PhoneFacsimile" optional="true" value="" size="25"/>
             <input type="text" name="DeliveryPoint" label="Delivery Point" id="DeliveryPoint" optional="true" value="" size="52"/>
             <input type="text" name="City" label="City" id="City" optional="true" value="" size="52"/>
             <input type="text" name="AdministrativeArea" label="Administrative Area" id="AdministrativeArea" optional="true" value="" size="52"/>
             <input type="text" name="PostalCode" label="Postal Code" id="PostalCode" optional="true" value="" size="52"/>
             <input type="text" name="Country" label="Country" id="Country" optional="true" value="" size="52"/>
             <input type="text" name="ElectronicMailAddress" label="Electronic Mail Address" id="ElectronicMailAddress" optional="true" value="" size="52"/>
        </group>
   </section>
   <requestInformations>
        <buttons>
            <!--button name="resetButton" id="resetButton" label="Reset" iconImage="" onclick="tbxCat.Reset"/-->
            <button name="createWPSButton" id="createWPSButton" label="Create WPS Service" iconImage="" onclick="createServiceRequest"/>
        </buttons>
    </requestInformations>
</inputInterface>
