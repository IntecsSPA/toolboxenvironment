<inputInterface xmlns="http://gisClient.pisa.intecs.it/gisClient" localization="yes" pathLocalization="resources/localization/login/">
    <section name="PEPManagerAuthentication" cols="1">
        <group label="groupLogin">
            <input id="user" name="user" type="text" label="labelUser"/>
            <input id="password" name="password" type="password" label="labelPassword"/>
            <input id="messageLabel" name="messageLabel" type="label" value=""/>
        </group>
    </section>
    <requestInformation backgroundColor="#dfe8f6">
        <buttons>
            <button name="loginButton" id="loginButton" label="loginButton" iconImage="" onclick="pepManager.loginInterface.onLogin"/>
        </buttons>    
    </requestInformation>
</inputInterface>